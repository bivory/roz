{
  "$schema": "https://json.schemastore.org/jscpd.json",
  "threshold": 8,
  "reporters": ["console"],
  "ignore": [
    "**/target/**",
    "**/.git/**",
    "**/tests/**",
    "**/*.lock",
    "**/*.min.js"
  ],
  "format": ["rust", "markdown", "yaml", "toml", "sql"],
  "minLines": 5,
  "minTokens": 50,
  "skipLocal": false,
  "absolute": false,
  "gitignore": true
}
